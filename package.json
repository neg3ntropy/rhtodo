    "@types/elasticsearch": "^5.0.28",
    "@types/http-aws-es": "^1.1.3",
    "expect.js": "0.3.1",
    "mocha": "5.2.0",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
